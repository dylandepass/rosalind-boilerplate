{
  "name": "@rosalind/rosalind-template",
  "private": true,
  "version": "1.0.0",
  "description": "Starter franklin project with the Rosalind Template",
  "scripts": {
    "test": "wtr \"./test/**/*.test.js\" --node-resolve --port=2000 --coverage",
    "test:watch": "npm test -- --watch",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'blocks/**/*.css' 'styles/*.css'",
    "lint": "npm run lint:js && npm run lint:css",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "dev": "concurrently \"npm run franklin:up\" \"npm run postcss:dev\"",
    "franklin:up": "npx @adobe/helix-cli up",
    "postcss:dev": "gulp dev",
    "postcss:build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylandepass/rosalind-template.git"
  },
  "author": "Adobe",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/dylandepass/rosalind-template/issues"
  },
  "homepage": "https://github.com/dylandepass/rosalind-template#readme",
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/eslint-parser": "7.19.1",
    "@dylandepass/franklin-storybook-addon": "0.0.30",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@etchteam/storybook-addon-css-variables-theme": "^1.4.0",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/html": "6.5.15",
    "@web/test-runner": "0.14.0",
    "@web/test-runner-commands": "0.6.5",
    "chai": "4.3.6",
    "concurrently": "7.5.0",
    "cssnano": "5.1.14",
    "eslint": "8.24.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "gulp": "4.0.2",
    "gulp-header": "^2.0.9",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-util": "3.0.8",
    "merge-stream": "2.0.0",
    "postcss": "8.4.18",
    "postcss-import": "14.1.0",
    "postcss-mixins": "9.0.2",
    "postcss-preset-env": "7.8.2",
    "sinon": "14.0.1",
    "style-dictionary": "3.7.1",
    "stylelint": "14.14.0",
    "stylelint-config-standard": "29.0.0",
    "token-transformer": "0.0.28"
  }
}
