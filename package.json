{
  "name": "@rosalind/rosalind-template",
  "private": true,
  "version": "1.0.0",
  "description": "Starter franklin project with the Rosalind Template",
  "scripts": {
    "test": "wtr \"./test/**/*.test.js\" --node-resolve --port=2000 --coverage",
    "test:watch": "npm test -- --watch",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'blocks/**/*.post.css' 'styles/*.post.css' --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook && npx http-server storybook-static --port 6006\" \"wait-on http://127.0.0.1:6006 && npm run test-storybook\"",
    "build-storybook": "storybook build -o ./storybook && node ./bin/no-jekyll.js",
    "dev": "concurrently \"npm run franklin:up\" \"npm run postcss:dev\"",
    "franklin:up": "npx @adobe/helix-cli up",
    "postcss:dev": "gulp dev",
    "postcss:build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylandepass/rosalind-template.git"
  },
  "author": "Adobe",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/dylandepass/rosalind-template/issues"
  },
  "homepage": "https://github.com/dylandepass/rosalind-template#readme",
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@dylandepass/franklin-storybook-addon": "0.0.42",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@storybook/addon-a11y": "7.0.0-beta.36",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "7.0.0-beta.36",
    "@storybook/addon-interactions": "7.0.0-beta.36",
    "@storybook/addon-links": "7.0.0-beta.36",
    "@storybook/blocks": "7.0.0-beta.36",
    "@storybook/builder-vite": "^7.0.0-beta.36",
    "@storybook/html": "7.0.0-beta.36",
    "@storybook/html-vite": "^7.0.0-beta.36",
    "@storybook/jest": "0.0.10",
    "@storybook/test-runner": "^0.9.4",
    "@storybook/testing-library": "0.0.14-next.1",
    "@web/test-runner": "0.14.0",
    "@web/test-runner-commands": "0.6.5",
    "chai": "4.3.6",
    "concurrently": "7.5.0",
    "cssnano": "5.1.14",
    "dedent": "^0.7.0",
    "eslint": "8.24.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "gulp": "4.0.2",
    "gulp-header": "^2.0.9",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-util": "3.0.8",
    "merge-stream": "2.0.0",
    "postcss": "8.4.18",
    "postcss-import": "14.1.0",
    "postcss-mixins": "9.0.2",
    "postcss-preset-env": "7.8.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sinon": "14.0.1",
    "storybook": "^7.0.0-beta.36",
    "style-dictionary": "3.7.1",
    "stylelint": "14.14.0",
    "stylelint-config-standard": "29.0.0",
    "token-transformer": "0.0.28",
    "vite": "^4.0.4"
  }
}
